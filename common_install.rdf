<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:aboutURL>chrome://useragentswitcher/content/about/about.xul</em:aboutURL>
    <em:creator>@author@</em:creator>
    <em:description>@description@</em:description>
    <em:homepageURL>@home.page@</em:homepageURL>
    <em:iconURL>chrome://useragentswitcher/content/logo.png</em:iconURL>
    <em:id>@id@</em:id>
    <em:multiprocessCompatible>true</em:multiprocessCompatible>
    <em:name>@name@</em:name>
    <em:optionsURL>chrome://useragentswitcher/content/options/options.xul</em:optionsURL>
    <em:type>2</em:type><!-- extension -->
    <em:unpack>true</em:unpack>
    <em:updateInfoURL>@home.page@releases/</em:updateInfoURL>
    <em:updateURL>https://cdn.rawgit.com/keithbowes/user-agent-switcher/master/update.rdf</em:updateURL>
    <em:version>@version@</em:version>
    <em:targetApplication>
      <Description>
        <em:id>toolkit@mozilla.org</em:id>
        <em:maxVersion>56.*</em:maxVersion>
        <em:minVersion>1.9b1</em:minVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
